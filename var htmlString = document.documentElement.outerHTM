function returnVal(key){
	htmlString = document.documentElement.outerHTML;
	a = htmlString.split("<a jsaction")[1];
	var a = a + "";
	var a = a.split('<span class=')[0];
	e = a.split(key + '="')[1] + "";
	return e.split('"')[0] + "";
}

